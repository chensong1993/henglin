@import "../loading_more/loading_more.wxss";

.main {
  background: #f1f4f8;
  padding: 20rpx 0;
  height: 100%;
  margin: 0;
}

.agency_main {
  background: white;
  margin:20rpx;
  clear: both;
  overflow: hidden;
  color: #a0a0a0;
  padding: 0 20rpx;
  font-size: 24rpx;
  border-radius: 10rpx;
}

.a_name {
  float: left;
  height: 60rpx;
  line-height: 60rpx;
  color: black;
  width: 150rpx;
  text-align: center;
}

.a_levenl {
  width: 200rpx;
  text-align: center;
  float: left;
  height: 60rpx;
  line-height: 60rpx;
}

.levenl {
  width: 200rpx;
  text-align: center;
  float: left;
  height: 60rpx;
  color: #65cb99;
  line-height: 60rpx;
}

.a_num {
  float: left;
  height: 60rpx;
  line-height: 60rpx;
  width: 150rpx;
  text-align: center;
}

.a_caozuo {
  float: right;
  height: 60rpx;
  line-height: 60rpx;
  width: 150rpx;
  text-align: center;
}

.list {
  background: white;
  border-radius: 10rpx;
  padding: 20rpx;
  margin: 20rpx;
  position: absolute;
  z-index: 99;
  left: 50%;
  top: 100rpx;
  margin-left: -240rpx;
}


.list_text {
  width: 400rpx;
  padding: 20rpx;
  text-align: center;
  border-bottom: 1rpx gainsboro solid;
}

.confirm {
  padding: 5rpx 10rpx;
  width: 80rpx;
  background: #5bc2cb;
  position: relative;
  left: 50%;
  text-align: center;
  margin-top: 40rpx;
  color: white;
  border-radius: 10rpx;
  margin-left: -40rpx;
}

.mengceng {
  filter: alpha(Opacity=80);
  -moz-opacity: 0.8;
  opacity: 0.8;
  background: black;
  z-index: 9;
  position: absolute;
  width: 100%;
  top: 0;
}

.faka_main {
  background: white;
  margin: 20rpx;
  clear: both;
  overflow: hidden;
  color: #a0a0a0;
  padding: 0 20rpx;
  font-size: 24rpx;
  border-radius: 10rpx;
}

.faka {
  border-radius: 10rpx;
  margin: 37rpx;
  position: absolute;
  z-index: 99;
  top: 80rpx;
  width: 90%;
  background: gainsboro;
}


.faka_name {
  float: left;
  height: 60rpx;
  line-height: 60rpx;
  color: black;
  width: 150rpx;
  text-align: center;
}

.faka_caozuo {
  float: right;
  height: 60rpx;
  line-height: 60rpx;
  width: 150rpx;
  text-align: center;
}
