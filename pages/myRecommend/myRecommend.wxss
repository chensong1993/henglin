.main {
  background: #f1f4f8;
  margin: 0;
  padding: 20rpx 0;
}

.agency_main {
  background: white;
  margin: 20rpx;
  clear: both;
  overflow: hidden;
  color: #a0a0a0;
  padding: 0 20rpx;
  font-size: 24rpx;
  border-radius: 10rpx;
}

.a_name {
  float: left;
  height: 60rpx;
  line-height: 60rpx;
  color: black;
  width: 150rpx;
  text-align: center;
}

.a_levenl {
  width: 200rpx;
  text-align: center;
  float: left;
  height: 60rpx;
  line-height: 60rpx;
}

.levenl {
  width: 200rpx;
  text-align: center;
  float: left;
  height: 60rpx;
  color: #65cb99;
  line-height: 60rpx;
}

.a_caozuo {
  float: right;
  height: 60rpx;
  line-height: 60rpx;
  width: 150rpx;
  text-align: center;
}

.list {
  background: white;
  border-radius: 10rpx;
  padding: 20rpx;
  margin: 20rpx;
  position: absolute;
  top: 50%;
  z-index: 99;
  left: 50%;
  margin-left: -240rpx;
  margin-top: -70rpx;
}

.list_text {
  width: 400rpx;
  padding: 20rpx;
  text-align: center;
  border-bottom: 1rpx gainsboro solid;
}

.confirm {
  padding: 5rpx 10rpx;
  width: 80rpx;
  background: #5bc2cb;
  position: relative;
  left: 50%;
  text-align: center;
  margin-top: 40rpx;
  color: white;
  border-radius: 10rpx;
  margin-left: -40rpx;
}

.mengceng {
  filter: alpha(Opacity=80);
  -moz-opacity: 0.8;
  opacity: 0.8;
  background: black;
  z-index: 9;
  position: absolute;
  width: 100%;
  top: 0;
}

.main_head {
  clear: both;
  overflow: hidden;
  height: auto;
  width: auto;
  margin: 20rpx;
  background: white;
  padding: 20rpx 20rpx;
  font-size: 30rpx;
  border-radius: 10rpx;
}

.img_icon {
  height: 115rpx;
  width: 115rpx;
  float: left;
  background: black;
  border-radius: 10rpx;
  margin-right: 20rpx;
}

.mine_head {
  float: left;
  color: black;
}

.data_1 {
  clear: both;
  overflow: hidden;
  margin-top: 0rpx;
}

.name {
  font-size: 40rpx;
  float: left;
  margin-right: 30rpx;
}

.phone {
  font-size: 32rpx;
  float: left;
  padding-top: 10rpx;
}

.data_2 {
  clear: both;
  overflow: hidden;
  margin-top: 10rpx;
}

.level {
  float: left;
  padding-top: 5rpx;
  color: black;
  font-size: 32rpx;
  font-weight: bold;
}
